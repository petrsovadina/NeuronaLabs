# Konfigurace úložiště
# Název bucketu pro ukládání souborů (DICOM, dokumenty, atd.)
SUPABASE_STORAGE_BUCKET=medical-files

# Další konfigurace Supabase
# Povolit/zakázat automatické připojení Realtime služby
# true: aktivní sledování změn, false: vypnuto pro úsporu zdrojů
SUPABASE_REALTIME_ENABLED=true

# Maximální počet opakování při selhání připojení
# Chrání před přechodným selháním sítě
SUPABASE_MAX_RETRY_ATTEMPTS=3

# Orthanc DICOM Server
# URL lokální nebo vzdálené instance Orthanc serveru
ORTHANC_URL=http://localhost:8042
ORTHANC_USERNAME=admin
ORTHANC_PASSWORD=your_orthanc_password

# Cesty pro ukládání DICOM souborů
# Absolutní cesta pro trvalé ukládání DICOM souborů
DICOM_STORAGE_PATH=/path/to/dicom/storage

# Dočasná cesta pro zpracování DICOM souborů
DICOM_TEMP_PATH=/path/to/dicom/temp

# Logging Configuration
# Úroveň logování aplikace
LOGGING_LEVEL=Information
# Cesta pro ukládání logů
LOGGING_PATH=/path/to/logs

# Performance and Caching
# Povolit/zakázat caching
CACHE_ENABLED=true
# Doba platnosti cache v minutách
CACHE_DURATION_MINUTES=60

# Monitoring
# Připojovací řetězec pro Application Insights
APPLICATIONINSIGHTS_CONNECTION_STRING=your_app_insights_connection_string

# External Services
# URL pro OHIF Viewer
OHIF_VIEWER_URL=https://viewer.neuronalabs.com

# Security
# Seznam povolených originů pro CORS
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://neuronalabs.com

# Feature Flags
# Povolit/zakázat anonymizaci DICOM souborů
FEATURE_DICOM_ANONYMIZATION=false
# Povolit/zakázat pokročilé vyhledávání
FEATURE_ADVANCED_SEARCH=true