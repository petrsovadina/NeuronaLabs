{
    "FirstRunExperience": {
        "RemindersEnabled": false
    },
    "Storage": {
        "Mode": "Disk",
        "Path": "/data"
    },
    "Ingestion": {
        "Port": 5341,
        "Tcp": {
            "Enabled": true
        },
        "Http": {
            "Enabled": true
        }
    },
    "Authentication": {
        "Mode": "None"
    },
    "WebConfig": {
        "BasePath": "/",
        "AllowLocalIpAddress": true
    },
    "Retention": {
        "Mode": "Rolling",
        "RetainedEventCount": 100000,
        "DaysToKeep": 30
    }
}
